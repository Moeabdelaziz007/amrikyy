{
  "system": {
    "name": "AuraOS MCP System",
    "version": "1.0.0",
    "description": "AI-powered code analysis and optimization system"
  },
  "agents": {
    "logic": {
      "enabled": true,
      "description": "Code structure analysis and logic optimization",
      "priority": "high"
    },
    "creativity": {
      "enabled": true,
      "description": "Innovative solutions and design patterns",
      "priority": "medium"
    },
    "optimization": {
      "enabled": true,
      "description": "Performance and efficiency improvements",
      "priority": "high"
    }
  },
  "memory": {
    "type": "redis",
    "host": "redis",
    "port": 6379,
    "database": 0,
    "timeout": 5
  },
  "bridge": {
    "host": "0.0.0.0",
    "port": 8080,
    "cors_enabled": true,
    "timeout": 30
  },
  "processing": {
    "parallel_processing": true,
    "max_concurrent_requests": 10,
    "request_timeout": 30,
    "retry_attempts": 3
  },
  "logging": {
    "level": "INFO",
    "rotation": "10 MB",
    "retention": "7 days",
    "format": "{time:YYYY-MM-DD HH:mm:ss} | {level} | {name}:{function}:{line} - {message}"
  },
  "features": {
    "real_time_analysis": true,
    "batch_processing": true,
    "caching": true,
    "health_monitoring": true,
    "metrics_collection": true
  }
}
