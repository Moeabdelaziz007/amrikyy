{
  "repositories": [
    "../auraos-frontend",
    "../auraos-backend"
  ],
  "defaultValues": {
    "NODE_ENV": "development",
    "PORT": "3002",
    "FRONTEND_URL": "http://localhost:5173",
    "DATABASE_URL": "SET_YOUR_DATABASE_URL",
    "FIREBASE_PROJECT_ID": "SET_YOUR_FIREBASE_PROJECT_ID",
