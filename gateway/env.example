PORT=3001
NODE_ENV=development

# JWT Configuration
JWT_SECRET=your_jwt_secret_key_here
JWT_EXPIRES_IN=24h

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# External Services
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here
AUTOPILOT_API_KEY=your_autopilot_api_key_here

# API Keys for External Apps
EXTERNAL_APP_API_KEY=your_external_app_api_key_here

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Logging
LOG_LEVEL=info
LOG_FILE=logs/gateway.log