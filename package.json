{
  "name": "auraos-telegram-system",
  "version": "1.0.0",
  "description": "Complete AuraOS Telegram Bot System with AI, Automation, and Monitoring",
  "main": "unified-telegram-bot.js",
  "scripts": {
    "start": "npm run start:frontend",
    "start:frontend": "vite",
    "start:backend": "cd backend && npm run dev",
    "dev": "concurrently \"npm:start:frontend\" \"npm:start:backend\"",
    "test": "node telegram-test-suite.js",
    "dashboard": "node telegram-bot-dashboard.js",
    "deploy": "firebase deploy",
    "build": "vite build",
    "preview": "vite preview",
    "env:template": "npx ts-node ./scripts/env-agent.ts generate-template",
    "env:validate": "npx ts-node ./scripts/env-agent.ts validate",
    "env:scan": "npx ts-node ./scripts/env-agent.ts scan",
    "env:share:create": "npx ts-node ./scripts/env-share-agent.ts create",
    "env:share:sync": "npx ts-node ./scripts/env-share-agent.ts sync"
  },
  "keywords": [
    "telegram",
    "bot",
    "ai",
    "automation",
    "monitoring",
    "auraos",
    "nodejs",
    "typescript",
    "react"
  ],
  "author": "AuraOS Team",
  "license": "MIT",
  "dependencies": {
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-slot": "^1.2.3",
    "@tailwindcss/typography": "^0.5.19",
    "class-variance-authority": "^0.7.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^4.18.2",
    "firebase": "^10.7.1",
    "firebase-admin": "^12.0.0",
    "helmet": "^7.1.0",
    "lucide-react": "^0.544.0",
    "node-telegram-bot-api": "^0.66.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/react": "^19.1.15",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "nodemon": "^3.0.2",
    "postcss": "^8.5.6",
    "terser": "^5.44.0",
    "typescript": "^5.9.2",
    "vite": "^5.0.8"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/auraos/telegram-system"
  },
  "bugs": {
    "url": "https://github.com/auraos/telegram-system/issues"
  },
  "homepage": "https://github.com/auraos/telegram-system#readme"
}
