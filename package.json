{
  "name": "amrikyy-aios-system",
  "version": "1.0.0",
  "description": "Amrikyy AIOS System - Advanced AI-Powered Operating System with intelligent agents and automation",
  "main": "index.js",
  "scripts": {
    "start": "./start-a2a-system.sh",
    "dev": "docker-compose -f docker-compose.dev.yml up -d",
    "stop": "docker-compose -f docker-compose.dev.yml down",
    "clean": "docker-compose -f docker-compose.dev.yml down -v",
    "logs": "docker-compose -f docker-compose.dev.yml logs -f",
    "test": "npm run test:gateway && npm run test:integration",
    "test:gateway": "cd gateway && npm test",
    "test:integration": "cd tests && npm test",
    "test:watch": "cd gateway && npm run test:watch",
    "build": "vite build",
    "build:gateway": "cd gateway && npm run build",
    "preview": "vite preview",
    "server": "node server.js",
    "start:frontend": "npm run build && npm run server",
    "lint": "cd gateway && npm run lint",
    "lint:fix": "cd gateway && npm run lint:fix",
    "docs": "cd docs && npm run serve",
    "health": "curl -f http://localhost:3001/api/health || echo 'Service not ready'",
    "status": "docker-compose -f docker-compose.dev.yml ps",
    "restart": "npm run stop && npm run dev",
    "update": "docker-compose -f docker-compose.dev.yml pull && docker-compose -f docker-compose.dev.yml up -d --build"
  },
  "keywords": [
    "aios",
    "ai",
    "operating-system",
    "automation",
    "intelligent-agents",
    "modern-ui",
    "amrikyy",
    "aios-system"
  ],
  "author": "Amrikyy AIOS Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/amrikyy/aios-system.git"
  },
  "bugs": {
    "url": "https://github.com/amrikyy/aios-system/issues"
  },
  "homepage": "https://amrikyy-aios.web.app",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-hover-card": "^1.1.15",
